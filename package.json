{
  "name": "users",
  "version": "1.0.0",
  "description": "",
  "main": "src/main/server.js",
  "scripts": {
    "start": "yarn build:ts && fastify start -l info dist/app.js",
    "build:ts": "tsc",
    "test": "jest --colors --noStackTrace --no-cache --runInBand ",
    "test-cov": "jest --colors --noStackTrace --no-cache --runInBand --coverage",
    "test-watch": "jest --no-cache --watch",
    "test-mutation": "yarn stryker run",
    "check-dependencies": "npm-check --strict -s -u",
    "lint": "eslint --fix --ignore-path .eslintignore --ext .js,.ts .",
    "dev": "tsc && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"tsc -w\" \"fastify start --ignore-watch=.ts$ -w -l info -P dist/main/server.js\"",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@stryker-mutator/core": "^5.6.1",
    "@stryker-mutator/jest-runner": "^5.6.1",
    "@types/jest": "^27.4.1",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^17.0.10",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "commitizen": "^4.2.4",
    "concurrently": "^7.0.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fastify-tsconfig": "^1.0.1",
    "husky": "^7.0.0",
    "jest": "^27.4.7",
    "lint-staged": "^12.3.7",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "_moduleAliases": {
    "@core": "dist/core",
    "@main": "dist/main",
    "@config": "dist/core/config"
  },
  "dependencies": {
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "fastify": "^3.26.0",
    "fastify-autoload": "^3.10.0",
    "fastify-cors": "^6.0.2",
    "fastify-plugin": "^3.0.0",
    "inversify": "^6.0.1",
    "module-alias": "^2.2.2",
    "reflect-metadata": "^0.1.13",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
